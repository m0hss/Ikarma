}
QScrollBar:horizontal {
  height: 16px;
  margin: 2px 16px 2px 16px;
  border: 1px solid rgba(254, 44, 85, 100);
  border-radius: 4px;
  background-color: rgb(185, 185, 185); }

QScrollBar:vertical {
  background-color: rgb(185, 185, 185);
  width: 16px;
  margin: 16px 2px 16px 2px;
  border: 1px solid rgba(254, 44, 85, 100);
  border-radius: 4px; }

QScrollBar::handle:horizontal {
  background-color: rgba(254, 44, 85, 100);
  border: 1px solid rgba(254, 44, 85, 100);
  border-radius: 4px;
  min-width: 8px; }
  QScrollBar::handle:horizontal:hover {
    background-color:rgba(254, 44, 85, 1.0);
    border: rgba(254, 44, 85, 1.0);
    border-radius: 4px;
